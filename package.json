{
  "name": "pg-businessflow-ai-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for PG-BusinessFlow-AI Autonomous Governance System",
  "private": true,
  "workspaces": [
    "libs/*",
    "services/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf dist && rm -f *.tsbuildinfo",
    "build": "tsc -b libs/common-types && tsc-alias -p libs/common-types/tsconfig.json && tsc -b services/property-ai && tsc-alias -p services/property-ai/tsconfig.json",
    "start": "node dist/services/property-ai/main.js",
    "start:dev:property-ai": "node -r dotenv/config --require ts-node/register --require tsconfig-paths/register services/property-ai/src/main.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/firestore": "^8.0.0",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.9.3"
  }
}
